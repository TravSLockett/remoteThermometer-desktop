<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="loginWindow">
    <property name="cursor">based_arrow_down</property>
    <property name="height">300</property>
    <property name="padding">20</property>
    <property name="width">400</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <rows>
        <row id="3">
          <property name="pad">20</property>
          <property name="weight">0</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="ttk.Label" id="Label_2">
        <property name="anchor">center</property>
        <property name="compound">top</property>
        <property name="text" translatable="yes">Sign in/ Register</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="username">
        <property name="text" translatable="yes">Username</property>
        <property name="textvariable">string:username_value</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="Entry_1">
        <layout>
          <property name="column">0</property>
          <property name="padx">70</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_4">
        <property name="text" translatable="yes">Password</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="Entry_2">
        <property name="show">â€¢</property>
        <property name="validate">none</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="register">
        <property name="command">Register</property>
        <property name="compound">left</property>
        <property name="cursor">based_arrow_down</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Register</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="login">
        <property name="command">Login</property>
        <property name="text" translatable="yes">Sign in</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
